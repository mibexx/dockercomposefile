{
  "name"       : "mibexx/dockercomposefile",
  "description": "Read and write a docker composer file",
  "license"    : "MIT",
  "authors"    : [
    {
      "name" : "Mike Bertram",
      "email": "contact@mibexx.de"
    }
  ],
  "require"    : {
    "php"              : ">=5.6",
    "mibexx/collection": "~0.1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*"
  },
  "autoload"   : {
    "psr-4"                : {
      "mibexx\\dockercomposefile\\"      : "src",
      "mibexx\\dockercomposefile\\test\\": "tests"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
